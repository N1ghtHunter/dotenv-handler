  "author": "Mazin Islam <mazinislam431@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/N1ghtHunter/dotenv-handler/issues"
  },
  "homepage": "https://github.com/N1ghtHunter/dotenv-handler#readme",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.6",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.13.1"
  },
