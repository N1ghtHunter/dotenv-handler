{
  "name": "dotenv-handler",
  "version": "1.3.0",
  "description": "A lightweight helper for loading, setting, and saving environment variables using dotenv.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test": "jest",
    "lint": "eslint ./src/**/*.ts ./tests/**/*.ts",
    "format": "prettier --write ./src/**/*.ts ./tests/**/*.ts",
    "prepublishOnly": "pnpm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/N1ghtHunter/dotenv-handler.git"
